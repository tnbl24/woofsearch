<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/sua_bai_viet_container_edit_post"
  android:layout_width="match_parent"
  android:layout_height="807dp"
  android:background="@drawable/sua_bai_viet_container_edit_post">
  
  <ImageView
    android:id="@+id/sua_bai_viet_container_group"
    android:layout_width="393dp"
    android:layout_height="82dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/sua_bai_viet_container_group" />
  
  <TextView
