    android:layout_gravity="bottom">

    <TextView
      android:id="@+id/text_hinh_anh"
      android:layout_width="match_parent"
      android:layout_height="25.71dp"
      android:layout_marginLeft="37.98dp"
      android:layout_marginRight="0dp"
      android:layout_gravity="top"
      android:gravity="center_horizontal|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Hình ảnh"
      android:textColor="#FF000000"
      android:textSize="18dp" />

    <ImageView
        android:id="@+id/sua_bai_viet_image_group"
        android:layout_width="34.93dp"
        android:layout_height="29.25dp"
        android:layout_marginTop="0.32dp"
        android:layout_marginRight="95dp"
        android:layout_marginBottom="0dp"
        android:scaleType="centerInside"
        android:src="@drawable/sua_bai_viet_image_group" />
  </FrameLayout>
  
  <TextView
    android:id="@+id/sua_bai_viet_container_group1"
    android:layout_width="72dp"
    android:layout_height="25.71dp"
    android:layout_marginLeft="52dp"
    android:layout_marginTop="562dp"
    android:layout_gravity="top|left"
    android:gravity="center_horizontal|center_vertical"
    android:includeFontPadding="false"
    android:maxLines="1"
    android:singleLine="true"
    android:text="Video"
    android:textColor="#FF000000"
    android:textSize="18dp" />

