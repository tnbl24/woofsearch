      android:id="@+id/txt_friends_count"
      android:layout_width="147.38dp"
      android:layout_height="20dp"
      android:layout_marginLeft="21.83dp"
      android:layout_marginTop="19dp"
      android:layout_marginBottom="-1dp"
      android:layout_gravity="top|left"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Bạn bè của bạn (0) "
      android:textColor="#FF000000"
      android:textSize="15dp" />
    
    <TextView
      android:id="@+id/txt_arrow"
      android:layout_width="match_parent"
      android:layout_height="25dp"
      android:layout_marginLeft="354.79dp"
      android:layout_marginRight="22.93dp"
      android:layout_marginTop="16.5dp"
      android:layout_marginBottom="-0.5dp"
      android:layout_gravity="top"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="&gt;"
      android:textColor="#FF737373"
      android:textSize="20dp" />
    
    <ImageView
      android:id="@+id/img_rectangle3"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginTop="49dp"
      android:background="@drawable/img_rectangle3"
      android:scaleType="centerCrop" />
    
    <TextView
      android:id="@+id/txt_recent_activity"
      android:layout_width="120.08dp"
      android:layout_height="16dp"
      android:layout_marginLeft="21.83dp"
      android:layout_marginTop="61.5dp"
      android:layout_marginBottom="-0.5dp"
      android:layout_gravity="top|left"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Hoạt động gần đây"
      android:textColor="#FF737373"
      android:textSize="12dp" />
  </FrameLayout>
  
  <ImageView
    android:id="@+id/img_gear_solid"
    android:layout_width="30dp"
    android:layout_height="30dp"
    android:layout_marginLeft="344dp"
    android:layout_marginTop="46dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/img_gear_solid" />
</FrameLayout>
