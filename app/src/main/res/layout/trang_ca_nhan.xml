    android:gravity="left|center_vertical"
    android:includeFontPadding="false"
    android:maxLines="1"
    android:singleLine="true"
    android:text="Nam"
    android:textColor="#FF000000"
    android:textSize="15dp" />
  
  <TextView
    android:id="@+id/tv_ha_noi"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:layout_marginLeft="76dp"
    android:layout_marginTop="259.5dp"
    android:layout_marginBottom="-1dp"
    android:layout_gravity="top|left"
    android:gravity="left|center_vertical"
    android:includeFontPadding="false"
    android:maxLines="1"
    android:singleLine="true"
    android:text="Hà Nội"
    android:textColor="#FF000000"
    android:textSize="15dp" />
  
  <ImageView
    android:id="@+id/ic_house_solid"
    android:layout_width="20dp"
    android:layout_height="17.78dp"
    android:layout_marginLeft="47dp"
    android:layout_marginTop="258.5dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/ic_house_solid" />
  
  <ImageView
    android:id="@+id/iv_rectangle1"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_marginLeft="42dp"
    android:layout_marginTop="294.5dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/iv_img_rectangle1" />
  
  <ImageView
    android:id="@+id/iv_rectangle2"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_marginLeft="146dp"
    android:layout_marginTop="294.5dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/iv_img_rectangle2" />
  
  <ImageView
    android:id="@+id/iv_rectangle3"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_marginLeft="250dp"
    android:layout_marginTop="294.5dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/iv_img_rectangle3" />
  
  <ImageView
    android:id="@+id/image_mars_solid"
    android:layout_width="18dp"
    android:layout_height="20.57dp"
    android:layout_marginLeft="49dp"
    android:layout_marginTop="230.5dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/image_mars_solid" />
  
  <FrameLayout
    android:id="@+id/container_group2"
    android:layout_width="393dp"
    android:layout_height="306dp"
    android:layout_marginTop="421dp"
