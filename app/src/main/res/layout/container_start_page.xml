<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_start_page"
  android:layout_width="match_parent"
  android:layout_height="807dp"
  android:background="@drawable/container_start_page">
  
  <ImageView
    android:id="@+id/image_start_page"
    android:layout_width="525dp"
    android:layout_height="807dp"
    android:layout_gravity="top|left"
    android:scaleType="centerInside"
    android:src="@drawable/image_start_page" />
  
  <ImageView
    android:id="@+id/rectangle_start_page"
    android:layout_width="393dp"
    android:layout_height="256dp"
    android:layout_gravity="top|left"
    android:background="@drawable/rectangle_start_page"
    android:scaleType="centerInside" />
  
  <FrameLayout
    android:id="@+id/container_group"
    android:layout_width="336dp"
    android:layout_height="240dp"
    android:layout_marginLeft="28dp"
    android:layout_marginTop="231dp"
    android:layout_gravity="top|left"
    android:background="@drawable/container_group">
    
    <ImageView
      android:id="@+id/rectangle_group"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginLeft="7dp"
      android:layout_marginTop="14dp"
      android:background="@drawable/rectangle_group"
      android:scaleType="centerCrop" />
    
    <ImageView
      android:id="@+id/rectangle_group1"
      android:layout_width="329dp"
      android:layout_height="match_parent"
      android:layout_gravity="left"
      android:background="@drawable/rectangle_group1"
      android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/text_group"
        android:layout_width="316dp"
        android:layout_height="199dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="41dp"
        android:layout_marginRight="7dp"
        android:gravity="center_horizontal|top"
        android:includeFontPadding="false"
        android:lineHeight="60dp"
        android:text="WoofSearch - Trao yêu thương và nhận lại yêu thương!&quot;"
        android:textColor="#FF000000"
        android:textSize="36dp" />
  </FrameLayout>

</FrameLayout>
